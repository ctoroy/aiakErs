/***** Button *****/
.Button
{
	-nexa-border 		: 1px solid #ebeced;
	-nexa-padding 		: 0px 7px 0px 7px;
	background 		: #f5f5f6;
	cursor 			: pointer;
	color 			: #293139;
	letter-spacing 		: -1px;
	border-radius 		: 3px;
	font 			: 13px "SpoqaHanSansNeo-Regular";
}
